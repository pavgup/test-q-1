<p id="notice"><%= notice %></p>

<p>
  <b>Name:</b>
  <%= @user.name %>
</p>

<p>
  <b>Products:</b><br/>
  <% @user.product.each do |product| %>
    <%= product.name %><br/>
  <% end %>
</p>

<p>
  <b>Orders:</b><br/>
  <% @user.order.each do |order| %>
    <%= order.id %><br/>
  <% end %>
</p>



<%= link_to 'Edit', edit_user_path(@user) %> |
<%= link_to 'Back', users_path %>
